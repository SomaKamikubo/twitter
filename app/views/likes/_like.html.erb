<% if  current_user.like? tweet %>
  <%= link_to image_tag('heart_red.png', :size => '30x30'), tweet_likes_path(tweet.id), method: :delete, remote: true %>
<% else %>
  <%= link_to image_tag('heart_white.png',:size => '30x30'), tweet_likes_path(tweet.id), method: :post, remote: true %>
<% end %>
<%= tweet.likes.count%>